{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": 2,
    "strict": 0,
    "no-console": 0,
    "no-underscore-dangle": ["error", { "allowAfterThis": true }]
  },
  "env": {
    "mocha": true
  }
}
